if __name__ == '__main__':
    s = input()
    alnum = False
    alpha = False
    dig = False
    lower = False
    upper = False
    for i in s:
        if i.isalnum():
            alnum = True
        if i.isalpha():
            alpha = True
        if i.isdigit():
            dig = True
        if i.islower():
            lower = True
        if i.isupper():
            upper = True
    print(alnum)
    print(alpha)
    print(dig)
    print(lower)
    print(upper)
